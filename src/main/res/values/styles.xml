<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="AppTheme.Launcher" parent="@android:style/Theme.Holo.NoActionBar">
		<item name="android:windowBackground">@drawable/splash</item>
	</style>
    <style name="AppTheme" parent="@android:style/Theme.Holo">
	</style>
	<style name="ButtonS">
		<item name="android:textAllCaps">false</item>
		<item name="android:textColor">@color/colorAccent</item>
		<item name="android:background">@drawable/bg</item>
		<item name="android:layout_marginLeft">0dp</item>
		<item name="android:layout_marginRight">0dp</item>
		<item name="android:layout_marginTop">10dp</item>
		<item name="android:layout_marginBottom">10dp</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:layout_width">fill_parent</item>
	</style>
	<style name="ButtonS2" parent="ButtonS">
		<item name="android:layout_marginLeft">4dp</item>
		<item name="android:layout_marginRight">4dp</item>
		<item name="android:layout_marginTop">3dp</item>
		<item name="android:layout_marginBottom">8dp</item>
		<item name="android:layout_height">40dp</item>
	</style>
	<style name="ButtonS3" parent="ButtonS">
		<item name="android:layout_marginLeft">0dp</item>
		<item name="android:layout_marginRight">0dp</item>
		<item name="android:layout_marginTop">5dp</item>
		<item name="android:layout_marginBottom">2dp</item>
		<item name="android:layout_height">40dp</item>
	</style>
	<style name="TextV">
		<item name="android:gravity">top</item>
		<item name="android:textColor">@color/dT</item>
		<item name="android:paddingLeft">8dp</item>
		<item name="android:typeface">sans</item>
		<item name="android:textStyle">bold|italic</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_width">match_parent</item>
	</style>
	<style name="TextV2" parent="TextV">
		<item name="android:textColor">@color/bT</item>
		<item name="android:typeface">monospace</item>
	</style>
	<style name="TextV3" parent="TextV">
		<item name="android:textAllCaps">true</item>
		<item name="android:textColor">@color/abc</item>
		<item name="android:typeface">monospace</item>
	</style>
</resources>
